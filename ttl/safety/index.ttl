@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix motif: <https://ns.onnx.cloud/motif#> .
#################
# AI Safety / Alignment Motifs
# Purpose: RLHF, constitutional AI, guardrails, watermarking patterns
#################

motif:Safety a skos:Concept ;
  skos:inScheme motif:CategoryScheme ;
  skos:prefLabel "Safety / Alignment"@en ;
  skos:definition "Patterns for AI safety: RLHF, guardrails, robustness, watermarking." .
#################
# Safety Fingerprints
#################

motif:S_aligned_eq_1 a skos:Concept ;
  skos:inScheme motif:FingerprintScheme ;
  skos:prefLabel "S.aligned=1"@en ;
  skos:definition "Output aligned with human preferences or values." .

motif:S_guarded_eq_1 a skos:Concept ;
  skos:inScheme motif:FingerprintScheme ;
  skos:prefLabel "S.guarded=1"@en ;
  skos:definition "Output filtered or constrained by safety guardrails." .

motif:S_robust_eq_1 a skos:Concept ;
  skos:inScheme motif:FingerprintScheme ;
  skos:prefLabel "S.robust=1"@en ;
  skos:definition "Robust to adversarial inputs or distribution shift." .
#################
# RLHF Motifs
#################

motif:RewardModel a motif:Motif ;
  skos:prefLabel "Reward Model"@en ;
  motif:hasSignature motif:Sig_1_to_1 ;
  motif:hasCategory motif:Safety ;
  motif:hasFingerprint motif:S_aligned_eq_1 ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint ;
  rdfs:seeAlso <https://arxiv.org/abs/2203.02155> ;
  skos:definition "Learned reward model from human feedback." .

