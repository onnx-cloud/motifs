@prefix motif: <https://ns.onnx.cloud/motif#> .
##################################
# Security & Defense Domain Applications
# Maps security and defense applications to motifs
##################################

motif:FacialRecognition a motif:Application, skos:Concept ;
  skos:inScheme motif:ApplicationScheme ;
  skos:prefLabel "Facial Recognition"@en ;
  skos:definition "Identity verification and identification from facial features."@en ;
  motif:belongsTo motif:Security ;
  motif:primaryMotif motif:Linear ;
  motif:usesMotif motif:Linear ;
  motif:usesMotif motif:Residual ;
  motif:usesMotif motif:Mul ;
  motif:usesMotif motif:ReduceSum ;
  motif:relatedUseCase motif:DenseRetrieval .

