motif:NetworkOptimization a motif:Application, skos:Concept ;
  skos:inScheme motif:ApplicationScheme ;
  skos:prefLabel "Network Optimization"@en ;
  skos:definition "Optimizing network topology, routing, and resource allocation."@en ;
  motif:belongsTo motif:Telecommunications ;
  motif:primaryMotif motif:Gather ;
  motif:usesMotif motif:Gather ;
  motif:usesMotif motif:Scatter ;
  motif:usesMotif motif:Attention ;
  motif:usesMotif motif:Loop ;
  motif:relatedUseCase motif:MessagePassing .

