motif:AutonomousDriving a motif:Application, skos:Concept ;
  skos:inScheme motif:ApplicationScheme ;
  skos:prefLabel "Autonomous Vehicle Perception"@en ;
  skos:definition "End-to-end perception system for self-driving cars combining camera, LiDAR, and radar."@en ;
  motif:belongsTo motif:Automotive ;
  motif:primaryMotif motif:Attention ;
  motif:usesMotif motif:Attention ;
  motif:usesMotif motif:Concat ;
