motif:DefectDetection a motif:Application, skos:Concept ;
  skos:inScheme motif:ApplicationScheme ;
  skos:prefLabel "Visual Defect Detection"@en ;
  skos:definition "Automated quality inspection using computer vision to identify product defects."@en ;
  motif:belongsTo motif:Manufacturing ;
  motif:primaryMotif motif:Residual ;
  motif:usesMotif motif:Residual ;
  motif:usesMotif motif:ForkJoin ;
  motif:usesMotif motif:Attention ;
  motif:usesMotif motif:If ;
  motif:relatedUseCase motif:ObjectDetection .

