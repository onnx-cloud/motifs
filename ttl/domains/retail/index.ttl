motif:ProductRecommendation a motif:Application, skos:Concept ;
  skos:inScheme motif:ApplicationScheme ;
  skos:prefLabel "Product Recommendation"@en ;
  skos:definition "Personalized product suggestions based on user behavior and preferences."@en ;
  motif:belongsTo motif:Retail ;
  motif:primaryMotif motif:Attention ;
  motif:usesMotif motif:Attention ;
  motif:usesMotif motif:EmbeddingLookup ;
  motif:usesMotif motif:Mul ;
  motif:usesMotif motif:ReduceSum ;
  motif:relatedUseCase motif:CollaborativeFiltering .

