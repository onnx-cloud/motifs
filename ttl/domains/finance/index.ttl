  skos:prefLabel "Algorithmic Trading"@en ;
  skos:definition "Automated trading strategies using time-series prediction and market microstructure analysis."@en ;
  motif:belongsTo motif:Finance ;
  motif:primaryMotif motif:Scan ;
  motif:usesMotif motif:Scan ;
  motif:usesMotif motif:Attention ;
  motif:usesMotif motif:Loop ;
  motif:usesMotif motif:If ;
