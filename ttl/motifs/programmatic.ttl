# Programmatic / Meta / Recursive motif instances


motif:Call a motif:Motif ;
  skos:prefLabel "Call"@en ;
  motif:hasSignature motif:Sig_1_to_1 ;
  motif:hasCategory motif:Programmatic ;
  motif:hasSemantics motif:Sem_Call .

