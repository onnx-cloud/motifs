# Memory / State motif instances


motif:Read a motif:Motif ;
  skos:prefLabel "Read"@en ;
  motif:hasSignature motif:Sig_1_to_1 ;
  motif:hasCategory motif:Memory ;
  motif:hasFingerprint motif:M_Memory ;
  motif:hasSemantics motif:Sem_Read .

