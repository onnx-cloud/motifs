# Distributed & Specialized motif instances


motif:AllReduce a motif:Motif ;
  skos:prefLabel "AllReduce"@en ;
  motif:hasSignature motif:Sig_1_to_1 ;
  motif:hasCategory motif:Misc ;
  motif:hasSemantics motif:Sem_AllReduce ;
  motif:hasConstraint motif:Constraint_Boundary_Communication .

