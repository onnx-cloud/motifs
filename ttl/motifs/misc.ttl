# Miscellaneous / Utility motif instances


motif:IdentityResidual a motif:Motif ;
  skos:prefLabel "Identity Residual"@en ;
  motif:hasSignature motif:Sig_1_to_1 ;
  motif:hasCategory motif:Misc ;
  motif:hasSemantics motif:Sem_IdentityResidual .

