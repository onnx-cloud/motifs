@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix motif: <https://ns.onnx.cloud/motif#> .
@prefix ex: <https://ns.onnx.cloud/examples##> .

# Control / Conditional motif instances


motif:If a motif:Motif ;
  skos:prefLabel "If"@en ;
  motif:hasSignature motif:Sig_1_to_N_optional ;
  motif:hasCategory motif:Control ;
  motif:hasFingerprint motif:C_Control ;
  motif:hasSemantics motif:Sem_If .

motif:Switch a motif:Motif ;
  skos:prefLabel "Switch"@en ;
  motif:hasSignature motif:Sig_1_to_N_optional ;
  motif:hasCategory motif:Control ;
  motif:hasSemantics motif:Sem_Switch .

motif:EarlyExit a motif:Motif ;
  skos:prefLabel "Early Exit"@en ;
