# Linear / Algebraic motif instances


motif:Linear a motif:Motif ;
  skos:prefLabel "Linear"@en ;
  motif:hasSignature motif:Sig_1_to_1 ;
  motif:hasCategory motif:Linear ;
  motif:hasFingerprint motif:T_Topology ;
  motif:hasFingerprint motif:T_depth_plus_1 ;
  motif:hasSemantics motif:Sem_LinearMatMulAdd ;
  motif:example ex:linear_chain.fuse .

