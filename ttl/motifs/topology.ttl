# Topology / Composition motif instances


motif:Fork a motif:Motif ;
  skos:prefLabel "Fork"@en ;
  motif:hasSignature motif:Sig_1_to_N ;
  motif:hasCategory motif:Topology ;
  motif:hasFingerprint motif:T_width_plus_N ;
  motif:hasSemantics motif:Sem_Fork ;
  motif:example ex:fork_join.fuse .

