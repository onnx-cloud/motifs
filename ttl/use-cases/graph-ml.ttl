@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix motif: <https://ns.onnx.cloud/motif#> .
##################################
# Graph Machine Learning Use Cases
# Links graph neural network patterns to computation graph motifs
##################################

motif:MessagePassing a motif:UseCase, skos:Concept ;
  skos:inScheme motif:UseCaseScheme ;
  skos:prefLabel "Message Passing Neural Network"@en ;
  skos:definition "Iterative neighborhood aggregation where nodes exchange and aggregate messages along edges."@en ;
  motif:hasDomain motif:GraphML ;
  motif:primaryMotif motif:Scan ;
  motif:usesMotif motif:Scan ;
  motif:usesMotif motif:Gather ;
  motif:usesMotif motif:Scatter ;
  motif:usesMotif motif:ReduceSum ;
  motif:usesMotif motif:Loop .

motif:GraphAttention a motif:UseCase, skos:Concept ;
  skos:inScheme motif:UseCaseScheme ;
  skos:prefLabel "Graph Attention Network"@en ;
  skos:definition "Applying attention mechanism to weight neighbor contributions in graph convolution."@en ;
  motif:hasDomain motif:GraphML ;
  motif:primaryMotif motif:Attention ;
  motif:usesMotif motif:Attention ;
  motif:usesMotif motif:Gather ;
  motif:usesMotif motif:SoftmaxEdge ;
  motif:usesMotif motif:WeightedSum .

motif:GraphPooling a motif:UseCase, skos:Concept ;
