@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix motif: <https://ns.onnx.cloud/motif#> .
##################################
# Retrieval & Recommendation Use Cases
# Links information retrieval patterns to computation graph motifs
##################################

motif:DenseRetrieval a motif:UseCase, skos:Concept ;
  skos:inScheme motif:UseCaseScheme ;
  skos:prefLabel "Dense Passage Retrieval"@en ;
  skos:definition "Encoding queries and passages into dense vectors for similarity-based retrieval."@en ;
  motif:hasDomain motif:Recommender ;
  motif:primaryMotif motif:motif_VectorSearch ;
  motif:usesMotif motif:motif_VectorSearch ;
  motif:usesMotif motif:Linear ;
  motif:usesMotif motif:ReduceSum ;
  motif:usesMotif motif:Gather .

motif:RAGRetrieval a motif:UseCase, skos:Concept ;
  skos:inScheme motif:UseCaseScheme ;
  skos:prefLabel "Retrieval-Augmented Generation"@en ;
  skos:definition "Augmenting language model generation with retrieved context documents."@en ;
  motif:hasDomain motif:NLP ;
  motif:primaryMotif motif:Concat ;
  motif:usesMotif motif:Concat ;
  motif:usesMotif motif:motif_VectorSearch ;
  motif:usesMotif motif:Attention ;
  motif:usesMotif motif:Scan .

motif:TwoTowerRetrieval a motif:UseCase, skos:Concept ;
  skos:inScheme motif:UseCaseScheme ;
  skos:prefLabel "Two-Tower Retrieval Model"@en ;
  skos:definition "Separate encoders for queries and items with dot-product similarity for efficient retrieval."@en ;
