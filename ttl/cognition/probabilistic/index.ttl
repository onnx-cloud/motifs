@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix motif: <https://ns.onnx.cloud/motif#> .
#################
# Probabilistic / Bayesian Motifs
# Purpose: Patterns for uncertainty, sampling, variational inference
#################


motif:Probabilistic a skos:Concept ;
  skos:inScheme motif:CategoryScheme ;
  skos:prefLabel "Probabilistic / Bayesian"@en ;
  skos:definition "Patterns for sampling, inference, and handling uncertainty." .

motif:Sample a motif:Motif ;
  skos:prefLabel "Sample"@en ;
  motif:hasCategory motif:Probabilistic ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

motif:MCMC a motif:Motif ;
  skos:prefLabel "MCMC Loop"@en ;
  motif:hasCategory motif:Probabilistic ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

motif:VariationalInfer a motif:Motif ;
  skos:prefLabel "Variational Inference"@en ;
  motif:hasCategory motif:Probabilistic ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

motif:ImportanceSample a motif:Motif ;
  skos:prefLabel "Importance Sampling"@en ;
  motif:hasCategory motif:Probabilistic ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

motif:UncertaintyCalibration a motif:Motif ;
  skos:prefLabel "Uncertainty Calibration"@en ;
  motif:hasCategory motif:Probabilistic ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

