#################
# Compression / Efficiency Motifs
# Purpose: Model compression, distillation, pruning, quantization, NAS
#################

motif:Compression a skos:Concept ;
  skos:inScheme motif:CategoryScheme ;
  skos:prefLabel "Compression / Efficiency"@en ;
  skos:definition "Patterns for pruning, quantization, distillation, and architecture search." .

motif:PruneStructured a motif:Motif ;
  skos:prefLabel "Structured Pruning"@en ;
  motif:hasCategory motif:Compression ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

motif:PruneUnstructured a motif:Motif ;
  skos:prefLabel "Unstructured Pruning"@en ;
  motif:hasCategory motif:Compression ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

motif:QuantizeStatic a motif:Motif ;
  skos:prefLabel "Post-Training Quantization"@en ;
  motif:hasCategory motif:Compression ;
  motif:hasSemantics motif:Sem_hasSemantics ;
  motif:hasConstraint motif:Constraint_hasConstraint .

motif:QuantizeAware a motif:Motif ;
  skos:prefLabel "Quantization-Aware Training"@en ;
