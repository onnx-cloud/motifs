#################
# Explainable AI (XAI) Motifs
# Purpose: Patterns for interpretability, attribution, probing, concept extraction
#################

motif:XAI a skos:Concept ;
  skos:inScheme motif:CategoryScheme ;
  skos:prefLabel "Explainable AI"@en ;
  skos:definition "Patterns for model interpretability: attribution, probing, concept bottlenecks." .
#################
# XAI Fingerprints
#################

xai:X_interpretable_eq_1 a skos:Concept ;
  skos:inScheme motif:FingerprintScheme ;
  skos:prefLabel "X.interpretable=1"@en ;
  skos:definition "Output is human-interpretable (concepts, text, visualization)." .

xai:X_attribution_eq_1 a skos:Concept ;
  skos:inScheme motif:FingerprintScheme ;
  skos:prefLabel "X.attribution=1"@en ;
  skos:definition "Provides attribution scores for inputs or features." .

xai:X_probing_eq_1 a skos:Concept ;
  skos:inScheme motif:FingerprintScheme ;
  skos:prefLabel "X.probing=1"@en ;
  skos:definition "Analyzes internal representations via auxiliary classifiers." .
