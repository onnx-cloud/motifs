@fuse 1.2
@opset onnx 18
@domain examples.cookbook

# Demonstrates symbolic dims (N) and typed axes

node dynamic_sum(x: f32[N]) -> f32[1] {
  axes: i64[1] = [0]
  total = ReduceSum(x, axes, keepdims@=0)
  shape_one: i64[1] = [1]
  Reshape(total, shape_one)
}

@proof node test_dynamic() {
  out = dynamic_sum([1.0, 1.0])
  assert out == [2.0]
}