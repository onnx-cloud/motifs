# Derived-from relationships for a motif (may be multi-valued)
PREFIX motif: <https://ns.onnx.cloud/motif#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT DISTINCT ?derivedFrom ?derivedFromLabel
WHERE {
  ?targetMotif a motif:Motif ;
               motif:derivedFrom ?derivedFrom .
  OPTIONAL { ?derivedFrom skos:prefLabel ?derivedFromLabel }
}
