# Find motif semantics and their usage count
PREFIX motif: <https://ns.onnx.cloud/motif#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT ?semantic (COUNT(?motif) AS ?count) (GROUP_CONCAT(?label; separator=", ") AS ?motifLabels)
WHERE {
  ?motif a motif:Motif ;
    skos:prefLabel ?label ;
    motif:hasSemantics ?semantic .
}
GROUP BY ?semantic
ORDER BY DESC(?count)
