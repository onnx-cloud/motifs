# Analyze motif signatures across the ontology
PREFIX motif: <https://ns.onnx.cloud/motif#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT ?signature (COUNT(?motif) AS ?count) ?motifs
WHERE {
  ?motif a motif:Motif ;
    motif:hasSignature ?signature .
}
GROUP BY ?signature
ORDER BY DESC(?count)
